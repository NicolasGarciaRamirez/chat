{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "aws/aws-sdk-php": "^3.148.0",
        "beyondcode/laravel-websockets": "^1.6",
        "doctrine/dbal": "v2.9.3",
        "fideloper/proxy": "^4.4.0",
        "intervention/image": "^2.5.1",
        "laravel/framework": "^v6.18.35",
        "laravel/helpers": "^1.1",
        "laravel/socialite": "^v4.4.1",
        "laravel/tinker": "^v2.4.1",
        "laravel/ui": "^1.0",
        "pusher/pusher-php-server": "~3.0"
    },
    "require-dev": {
        "facade/ignition": "^1.16.3",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^v3.0.1",
        "phpunit/phpunit": "^8.5.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
